{
  "name": "react-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "start:open": "webpack-dev-server --mode development --open",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --mode production",
    "precommit": "lint-staged",
    "mongo": "cd ~/mongo/bin && ./mongod --dbpath ~/mongo-data/",
    "server": "nodemon server/server.js"
  },
  "lint-staged": {
    "*.{js,tsx,ts}": [
      "prettier --write --single-quote --no-bracket-spacing",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.14",
    "@types/ramda": "^0.26.9",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/sinon": "^7.0.13",
    "css-loader": "^3.4.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.17.1",
    "prettier": "^1.19.1",
    "sinon": "^7.3.2",
    "style-loader": "^0.20.2",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-webpack-plugin": "^1.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mobx": "^6.1.5",
    "mobx-react": "^7.1.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.3",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
